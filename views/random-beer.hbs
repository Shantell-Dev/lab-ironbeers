<h1>Random Beer Page</h1>
<p>This is the random beer page content.</p>
{{#each beers}}
    <div class="beer">
        <img src="{{ image_url }}" alt="{{ name }}" class="beer-image">
        <h2>{{ name }}</h2>
        <p class="tagline">{{ tagline }}</p>
        <p>{{ description }}</p>
    </div>
{{/each}}
